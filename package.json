{
  "name": "store-craft",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev && netlify dev",
    "netlify:dev": "netlify dev",
    "dev:server": "cross-env NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest --watch --collectCoverageOnlyFrom ''",
    "test:coverage": "jest --coverage",
    "lint": "next lint",
    "prepare": "husky install",
    "lhci:local": "npm run build && lhci autorun --collect.numberOfRuns=1",
    "graphql:schema": "graphql-codegen --config codegen.yml -r dotenv/config"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "graphql": "^15.8.0",
    "graphql-request": "^3.7.0",
    "keen-slider": "^6.6.3",
    "lodash": "^4.17.21",
    "next": "^12.0.10",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.3.1",
    "@graphql-codegen/typescript": "^2.4.2",
    "@graphql-codegen/typescript-graphql-request": "^4.3.3",
    "@graphql-codegen/typescript-operations": "^2.2.2",
    "@jackfranklin/test-data-bot": "^1.4.0",
    "@netlify/functions": "^0.10.0",
    "@testing-library/dom": "^8.11.2",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/faker": "^5.5.8",
    "@types/lodash": "^4.14.177",
    "@types/react": "^17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "autoprefixer": "^10.4.2",
    "babel-jest": "^27.4.6",
    "cross-env": "^7.0.3",
    "dotenv": "^14.1.0",
    "eslint": "^8.7.0",
    "eslint-config-next": "^12.0.8",
    "eslint-config-prettier": "^8.3.0",
    "faker": "^5.5.3",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.4.7",
    "lint-staged": "^12.1.7",
    "msw": "^0.36.4",
    "netlify-cli": "^9.13.6",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "react-test-renderer": "^17.0.2",
    "tailwindcss": "^3.0.15",
    "typescript": "^4.5.4",
    "whatwg-fetch": "^3.6.2"
  },
  "engines": {
    "npm": ">=8.1.0",
    "node": ">=16.13.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "*.{js,jsx,ts,tsx,css,md}": "prettier --write"
  }
}
