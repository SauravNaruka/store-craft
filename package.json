{
  "name": "store-craft",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:server": "cross-env NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest --watch --collectCoverageOnlyFrom ''",
    "lint": "next lint",
    "prepare": "husky install",
    "test:coverage": "jest --coverage",
    "lhci:local": "npm run build && lhci autorun --collect.numberOfRuns=1"
  },
  "dependencies": {
    "graphql": "^15.6.1",
    "graphql-request": "^3.6.1",
    "next": "11.1.2",
    "node-fetch": "^3.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@jackfranklin/test-data-bot": "^1.3.0",
    "@netlify/functions": "^0.7.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/faker": "^5.5.8",
    "@types/react": "17.0.27",
    "autoprefixer": "^10.3.7",
    "babel-jest": "^27.2.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "faker": "^5.5.3",
    "husky": "^7.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.2.5",
    "lint-staged": "^11.2.3",
    "msw": "^0.35.0",
    "postcss": "^8.3.9",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "react-test-renderer": "^17.0.2",
    "sass": "^1.42.1",
    "tailwindcss": "^2.2.16",
    "typescript": "4.4.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "*.{js,jsx,ts,tsx,css,md}": "prettier --write"
  }
}
